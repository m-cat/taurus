@objects: <"objects.over">
@tiles: <"tiles.over">

@types: {
    room: "room"
    empty: "empty"
}

normal: {
    type: @types.room

    wall_tile: @tiles.stone_wall
    floor_tile: @tiles.dirt_floor

    doors: [
        (@objects.wooden_door 1)
    ]
}

test: {
    type: @types.empty

    wall_tile: @tiles.stone_wall
}

special: {
    type: @types.room

    wall_tile: @tiles.stone_wall
    floor_tile: @tiles.stone_floor

    doors: [
        (@objects.wooden_door 1)
    ]
}
